{
  "ConfigImports": {
    "BaseSettings": "../../../../appsettings/appsettings.base.json"
  },

  "ApplicationName": "EleonsoftProxy",
  "NotificationApplication": "Admin",
  "ProxyRouter": {
    "Enabled": true,
    "UseEvents": true,
    "DemoDomains": [],
    "IgnoreSslValidation": true,
    "ForwardEmptyPath": true,
    "SystemPathes": [
      "/api",
      "/hubs",
      "/files",
      "/swagger",
      "/MiniProfiler",
      "/profiler",
      "/health"
    ],
    "Locations": [
      {
        "Path": "",
        "Type": 0,
        "DefaultRedirect": "/apps/admin",
        "SubLocations": [
          {
            "Path": "adminsrv",
            "Type": 2,
            "SourceUrl": "https://localhost:5001",
            "IsAuthorized": false
          },
          {
            "Path": "auth",
            "Type": 2,
            "SourceUrl": "https://localhost:5002/auth",
            "IsAuthorized": false
          },
          {
            "Path": "core",
            "Type": 2,
            "SourceUrl": "https://localhost:5003",
            "IsAuthorized": false
          },
          {
            "Path": "resources",
            "Type": 2,
            "SourceUrl": "https://localhost:5002/resources",
            "IsAuthorized": false
          },
          {
            "Path": "telemetry/grafana",
            "Type": 2,
            "SourceUrl": "http://localhost:3000",
            "IsAuthorized": false
          },
          {
            "Path": "",
            "Type": 0,
            "DefaultRedirect": "/apps/admin",
            "IsAuthorized": false
          },
          {
            "Path": "apps/admin",
            "Type": 1,
            "SourceUrl": "https://localhost:5003/ui/modules/host",
            "IsAuthorized": false
          },
          {
            "Path": "modules/layout",
            "Type": 2,
            "SourceUrl": "https://localhost:5001/ui/modules/primeng-layout",
            "IsAuthorized": false
          },
          {
            "Path": "modules/chat",
            "Type": 2,
            "SourceUrl": "https://localhost:5001/ui/modules/eleoncorechat",
            "IsAuthorized": false
          },
          {
            "Path": "modules/host-administration",
            "Type": 2,
            "SourceUrl": "https://localhost:5003/ui/modules/host-administration",
            "IsAuthorized": false
          },
          {
            "Path": "modules",
            "Type": 2,
            "SourceUrl": "https://localhost:5001/ui/modules",
            "IsAuthorized": false
          },
          {
            "Path": "assets",
            "Type": 2,
            "SourceUrl": "https://localhost:5001/ui/apps/admin/assets",
            "IsAuthorized": false
          }
        ]
      }
    ]
  },
  "App": {
    "Domain": "network-protected.com",
    "Authority": "https://localhost:5002/auth",
    "Authorities": [
      "https://network-protected.com/auth",
      "https://localhost:5002/auth"
    ]
  },
  "MultiTenancySettings": {
    "Enabled": false
  },
  "HealthChecks": {
    "UI": {
      "Enabled": true,
      "Path": "/health",
      "Navigates": [
        {
          "Name": "Swagger",
          "Path": "/swagger"
        },
        {
          "Name": "Profiler",
          "Path": "/profiler"
        },
        {
          "Name": "Clear Cache",
          "Path": "/clear-cache",
          "Enabled": true
        }
      ]
    },
    "ConfigurationCheck": {
      "Enabled": true,
      "ThrowExceptionOnInvalid": false,
      "Configurations": {
      }
    },
    "HttpCheck": {
      "Timeout": 60,
      "IgnoreSsl": true,
      "Urls": [
        {
          "Name": "Admin",
          "Url": "https://localhost:5001/api/health/health-checks",
          "GoodStatusCodes": [ 200 ]
        },
        {
          "Name": "Auth",
          "Url": "https://localhost:5002/api/health/health-checks",
          "GoodStatusCodes": [ 200 ]
        },
        {
          "Name": "Core",
          "Url": "https://localhost:5003/api/health/health-checks",
          "GoodStatusCodes": [ 200 ]
        }
      ]
    }
  },
  "EventBus": {
    "ProviderOptionsJson": {
      "RabbitMqOptions": {
        "DefaultQueuePrefix": "eleonsoft_proxy_server_queue"
      }
    }
  }
}
