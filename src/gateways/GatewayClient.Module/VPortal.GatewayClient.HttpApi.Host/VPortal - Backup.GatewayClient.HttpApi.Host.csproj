<Project Sdk="Microsoft.NET.Sdk.Web">
	<Import Project="$(SolutionDir)/common.props" />

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
     
    <ImplicitUsings>enable</ImplicitUsings>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="devcert.pfx" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="devcert.pfx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Authentication.Certificate" />
		<PackageReference Include="Sentry" />
		<PackageReference Include="Serilog.AspNetCore" />
		<PackageReference Include="Serilog.Sinks.Async" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" />
		<PackageReference Include="Microsoft.AspNetCore.DataProtection.StackExchangeRedis" />
		<!--<PackageReference Include="workspace.analysis" PrivateAssets="all" />-->

	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Volo.Abp.AspNetCore.Serilog" />
		<PackageReference Include="Volo.Abp.Identity.AspNetCore" />
		<PackageReference Include="Volo.Abp.Caching.StackExchangeRedis" />
		<PackageReference Include="Volo.Abp.Swashbuckle" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\SharedModules\IdentityModule\Eleon.Identity.Module.HttpApi.Client\Eleon.Identity.Module.HttpApi.Client.csproj" />
	  <ProjectReference Include="..\VPortal.GatewayClient.Host.Collector\VPortal.GatewayClient.Host.Collector.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="localhostcert.pfx">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	  <None Update="gatewaydevcert.pfx">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>
</Project>
