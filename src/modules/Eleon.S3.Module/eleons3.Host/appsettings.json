{
  "ConfigImports": {
    "BasePath": "C:/Workspace/configs",
    "Files": [
      "event-bus.json",
      "cache.json",
      "logging.json",
      "swagger.json",
      "health-check.json",
      "sql.json",
      "telemetry.json"
    ],
    "ValidateSections": [
      {
        "Section": "EventBus",
        "Validator": "EventBus"
      }
    ]
  },
  "Kestrel": {
    "AddServerHeader": false,
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5560",
        "Certificate": {
          "Subject": "localhost",
          "Store": "My",
          "Location": "CurrentUser",
          "AllowInvalid": true
        }
      }
    }
  },
  "ApplicationName": "eleons3",
  "ClientApplicationName": "Admin",
  "App": {
    "Name": "Eleons3 Host",
    "Domain": "https://localhost:4040",
    "Authority": "https://localhost:4040"
  },
  "SystemMessages": {
    "Enabled": true
  },
  "TenantManagement": {
    "EnableMultiTenancy": true
  },
  "MiniProfiler": {
    "Enable": "true"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "qENrrvF6ECRRq68C"
  },
  "EventBus": {
    "ProviderOptionsJson": {
      "RabbitMqOptions": {
        "DefaultQueuePrefix": "eleons3_server_queue",
        "VirtualHost": "immu"
      }
    }
  },
  "ErrorHandling": {
    "IsFriendlyErrors": false,
    "DefaultErrorStatusCode": 600,
    "DefaultErrorPageTitle": "EleonS3 Error Page"
  },
  "EnableDebug": false,
  "HealthChecks": {
    "Enabled": true,
    "ApplicationName": "eleons3",
    "CheckTimeout": 5,
    "EnableDiagnostics": false,
    "RestartEnabled": false,
    "RestartRequiresAuth": true,
    "PublishOnFailure": true,
    "PublishOnChange": false,
    "PublishIntervalMinutes": 5,
    "SqlServer": {
      "EnableDiagnostics": false,
      "DiagnosticsCacheMinutes": 10,
      "MaxTablesInDiagnostics": 100
    },
    "Publishing": {
      "PublishOnFailure": true,
      "PublishOnChange": false,
      "PublishIntervalMinutes": 5,
      "MaxPublishesPerMinute": 1
    },
    "UI": {
      "Enabled": true,
      "Path": "/",
      "Navigates": [
        {
          "Name": "Swagger",
          "Path": "/swagger"
        },
        {
          "Name": "Profiler",
          "Path": "/profiler"
        }
      ]
    },
    "ConfigurationCheck": {
      "Enabled": true,
      "ThrowExceptionOnInvalid": false,
      "Configurations": {
      }
    },
    "HttpCheck": {
      "ClientName": "HealthCheckClient",
      "Timeout": 120,
      "IgnoreSsl": true
    },
    "DiskSpaceCheck": {
      "Items": [
        {
          "Path": "Logs",
          "MaxSizeMb": 500
        }
      ]
    }
  }
}
